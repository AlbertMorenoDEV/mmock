{
    "request": {
        "method": "POST",
        "path": "/users/*",
        "body": "*"
    },
	"response": {
        "statusCode": 202,
        "headers": {
			"Content-Type":["application/json"]
		},
		"body": "{{ request.body }}",
        "bodyAddition": "{ \"id\": {{request.url.users/(?P<Value>\\d+)}} }"
    },
    "persist" : {
		"fileName" : "/users/user-{{request.url.users/(?P<Value>\\d+)}}",
        "delete": false
	}
}