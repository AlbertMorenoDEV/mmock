{
    "request": {
        "method": "POST",
        "path": "/users/*",
        "body": "*"
    },
	"response": {
        "statusCode": 202,
        "headers": {
			"Content-Type":["application/json"]
		},
		"body": "{{ request.body }}",
        "bodyAppend": "{ \"id\": {{request.url.users/(?P<Value>\\d+)}}, \"city\": \"{{ fake.City }}\" }"
    },
    "persist" : {
		"name" : "/users/user-{{request.url.users/(?P<Value>\\d+)}}.json",
        "delete": false
	}
}