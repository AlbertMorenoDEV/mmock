{
    "request": {
        "method": "GET",
        "path": "/users/*"
    },
	"response": {
        "statusCode": 200,
        "headers": {
			"Content-Type":["application/json"]
		},
		"persisted": {
            "name" : "/users/user-{{request.url.users/(?P<Value>\\d+)}}.json",
            "notFound": {
                "statusCode": 404,
                "body": "File /users/user-{{request.url.users/(?P<Value>\\d+)}}.json Not Found"
            },
            "bodyAppend": "{ \"id\": {{request.url.users/(?P<Value>\\d+)}} }"
        }
    }
}